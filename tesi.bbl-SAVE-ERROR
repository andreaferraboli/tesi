      \strng{namehash}{ae2d920c38a36f43b8662476ba6c3d30}
      \strng{fullhash}{1f511667d223ab16f38bef3b57e0c89b}
      \strng{fullhashraw}{1f511667d223ab16f38bef3b57e0c89b}
      \strng{bibnamehash}{1f511667d223ab16f38bef3b57e0c89b}
